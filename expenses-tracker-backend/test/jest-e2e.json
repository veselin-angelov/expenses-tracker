{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@libs/ocr/(.*)": "<rootDir>/../libs/ocr/src/$1",
    "@libs/ocr": "<rootDir>/../libs/ocr/src"
  }
}